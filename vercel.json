{
    "version": 2,
    "builds": [
      {
        "src": "backend/src/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "user-frontend/build",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/src/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "user-frontend/build/$1"
      }
    ]
  }
  